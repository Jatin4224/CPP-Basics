#include <iostream>
using namespace std;

int main() {
  int arr[5];

  // Prompt the user to enter values for array elements
  cout << "Enter five integers: ";
  cin >> arr[0] >> arr[1] >> arr[2] >> arr[3] >> arr[4];

  //modifying
  arr[3] = 10;
  // Print the value of arr[3]
  cout << "Value at index 3: " << arr[3] << endl;

  return 0;
}

// 2d array
int main() {
  int arr[3][5];

//assigning
arr[1][3] = 78;
cout << arr[1][3];
  return 0;
}
