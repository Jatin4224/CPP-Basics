// if/else
//   Q1)wrtie a program that takes an input of age and prints if you are adult or not
// 

#include <iostream>
using namespace std;



int main() {
  int age;
  cin >> age;
  if (age >= 18) {
    cout << "adult" << endl;
  } else {
    cout << "not adult " << endl;
  }
  return 0;
}


